{
  "name": "unified-browser-platform",
  "version": "1.0.0",
  "description": "Real-time browser automation platform combining browser-use AI agent with WebRTC streaming",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "start:clean": "node src/server-clean.js",
    "dev": "nodemon src/server.js",
    "dev:clean": "nodemon src/server-clean.js",
    "install-python": "cd python-agent && pip install -e .",
    "setup": "npm install && npm run install-python",
    "setup:chrome": "node scripts/install-chrome.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:complete": "node setup.js",
    "postinstall": "node scripts/setup-env.js || echo 'Setup script completed with warnings'",
    "test": "npm run test:js && npm run test:python",
    "test:js": "jest",
    "test:python": "cd python-agent && python -m pytest",
    "test:platform": "node test-platform.js",
    "test:agent": "node validate-agent.js",
    "validate": "npm run test:platform && npm run test:agent",
    "lint": "eslint src/ && cd python-agent && ruff .",
    "format": "prettier --write src/ && cd python-agent && black .",
    "build": "webpack --mode production",
    "docker:build": "docker build -t unified-browser-platform .",
    "docker:run": "docker run -p 3000:3000 -p 8080:8080 unified-browser-platform"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ffmpeg-static": "^5.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.0.0",
    "puppeteer": "^23.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "screenshot-desktop": "^1.15.2",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "uuid": "^10.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "browser-automation",
    "ai-agent",
    "webrtc",
    "streaming",
    "puppeteer",
    "real-time",
    "remote-browser",
    "browser-use",
    "headless",
    "interactive"
  ],
  "author": "Unified Browser Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/unified-browser-platform.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/unified-browser-platform/issues"
  },
  "homepage": "https://github.com/yourusername/unified-browser-platform#readme"
}
